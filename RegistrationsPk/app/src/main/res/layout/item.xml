<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
    <androidx.cardview.widget.CardView android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginBottom="20dp">
        <LinearLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content">
            <LinearLayout android:gravity="center" android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="50dp" android:layout_marginLeft="10dp" android:layout_marginTop="20dp" android:layout_marginRight="10dp">
                <TextView android:textSize="14dp" android:textStyle="bold" android:layout_width="120dp" android:layout_height="wrap_content" android:text="Institute Name"/>
                <TextView android:textSize="15dp" android:textStyle="bold" android:id="@+id/txtschoolRegistrationForFreshInstituteName" android:layout_width="match_parent" android:layout_height="25dp" android:text=" " android:singleLine="true" android:inputType="text"/>
            </LinearLayout>
            <LinearLayout android:gravity="center" android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="50dp" android:layout_marginLeft="10dp" android:layout_marginRight="10dp">
                <TextView android:textSize="14dp" android:textStyle="bold" android:layout_width="120dp" android:layout_height="wrap_content" android:text="Address"/>
                <TextView android:textSize="15dp" android:textStyle="bold" android:id="@+id/txtschoolRegistrationForFreshAddress" android:layout_width="match_parent" android:layout_height="25dp" android:text=" " android:singleLine="true" android:inputType="text"/>
            </LinearLayout>
            <LinearLayout android:gravity="center" android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="50dp" android:layout_marginLeft="10dp" android:layout_marginRight="10dp">
                <TextView android:textSize="14dp" android:textStyle="bold" android:layout_width="120dp" android:layout_height="wrap_content" android:text="Tehsil"/>
                <TextView android:textSize="15dp" android:textStyle="bold" android:id="@+id/txtschoolRegistrationForFreshTehsil" android:layout_width="match_parent" android:layout_height="25dp" android:text=" " android:singleLine="true" android:inputType="text"/>
            </LinearLayout>
            <LinearLayout android:gravity="center" android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="50dp" android:layout_marginLeft="10dp" android:layout_marginRight="10dp">
                <TextView android:textSize="14dp" android:textStyle="bold" android:layout_width="120dp" android:layout_height="wrap_content" android:text="District"/>
                <TextView android:textSize="15dp" android:textStyle="bold" android:id="@+id/txtschoolRegistrationForFreshDistrict" android:layout_width="match_parent" android:layout_height="25dp" android:text=" " android:singleLine="true" android:inputType="text"/>
            </LinearLayout>
            <LinearLayout android:gravity="center" android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="50dp" android:layout_marginLeft="10dp" android:layout_marginRight="10dp">
                <TextView android:textSize="14dp" android:textStyle="bold" android:layout_width="120dp" android:layout_height="wrap_content" android:text="Owner Name"/>
                <TextView android:textSize="15dp" android:textStyle="bold" android:id="@+id/txtschoolRegistrationForFreshOwnerName" android:layout_width="match_parent" android:layout_height="25dp" android:text=" " android:singleLine="true" android:inputType="text"/>
            </LinearLayout>
            <LinearLayout android:gravity="center" android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="50dp" android:layout_marginLeft="10dp" android:layout_marginRight="10dp">
                <TextView android:textSize="14dp" android:textStyle="bold" android:layout_width="120dp" android:layout_height="wrap_content" android:text="Principal Name"/>
                <TextView android:textSize="15dp" android:textStyle="bold" android:id="@+id/txtschoolRegistrationForFreshPrincipalName" android:layout_width="match_parent" android:layout_height="25dp" android:text=" " android:singleLine="true" android:inputType="text"/>
            </LinearLayout>
            <LinearLayout android:gravity="center" android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="50dp" android:layout_marginLeft="10dp" android:layout_marginRight="10dp">
                <TextView android:textSize="14dp" android:textStyle="bold" android:layout_width="120dp" android:layout_height="wrap_content" android:text="School Level"/>
                <TextView android:textSize="15dp" android:textStyle="bold" android:id="@+id/txtschoolRegistrationForFreshSchoolLevel" android:layout_width="match_parent" android:layout_height="25dp" android:text=" " android:singleLine="true" android:inputType="text"/>
            </LinearLayout>
            <LinearLayout android:gravity="center" android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="50dp" android:layout_marginLeft="10dp" android:layout_marginRight="10dp">
                <TextView android:textSize="14dp" android:textStyle="bold" android:layout_width="120dp" android:layout_height="wrap_content" android:text="WhatsAPP PH."/>
                <TextView android:textSize="15dp" android:textStyle="bold" android:id="@+id/txtschoolRegistrationForFreshWhatsAppPH" android:layout_width="match_parent" android:layout_height="25dp" android:text=" " android:singleLine="true" android:inputType="number"/>
            </LinearLayout>
            <LinearLayout android:gravity="center" android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="50dp" android:layout_marginLeft="10dp" android:layout_marginRight="10dp">
                <TextView android:textSize="14dp" android:textStyle="bold" android:layout_width="120dp" android:layout_height="wrap_content" android:text="E-Mail"/>
                <TextView android:textSize="15dp" android:textStyle="bold" android:id="@+id/txtschoolRegistrationForFreshEMail" android:layout_width="match_parent" android:layout_height="25dp" android:text=" " android:singleLine="true" android:inputType="textEmailAddress"/>
            </LinearLayout>
        </LinearLayout>
    </androidx.cardview.widget.CardView>
</LinearLayout>
